
package com.javacodegeeks.enterprise.ws;


import javax.jws.WebService;

import com.reception.CL_reception;

@WebService(endpointInterface = "com.javacodegeeks.enterprise.ws.WebServiceInterface")
public class WebServiceImpl implements WebServiceInterface{
	@Override
	public String printMessage() {
		//new CL_reception();
		
		return new CL_reception().Hello();
	}
	@Override
    public String hello(String txt) {
        return "Hello " + txt + " !";
    }


}
